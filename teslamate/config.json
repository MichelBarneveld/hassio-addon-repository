{
    "name": "TeslaMate",
    "version": "1.17.1",
    "slug": "teslamate",
    "description": "A self-hosted data logger for your Tesla.",
    "arch": ["amd64"],
    "startup": "application",
    "boot": "auto",
    "ports": {
        "8099/tcp": null
    },
    "ports_description": {
        "8099/tcp": "Web interface. Disable this to use Home Assistant ingress."
    },
    "options": {
        "database_user": "user",
        "database_pass": "pass",
        "database_name": "abcd-postgres",
        "database_ssl": false,
        "database_port": 5432,
        "disable_mqtt": false,
        "mqtt_host": "core-mosquitto",
        "mqtt_user": "user",
        "mqtt_pass": "pass",
        "mqtt_tls": true,
        "mqtt_tls_accept_invalid_certs": false,
        "mqtt_namespace": "str",
        "timezone": "Europe/London"
    },
    "schema": {
        "database_user": "str",
        "database_pass": "str",
        "database_name": "str",
        "database_port": "int",
        "database_ssl": "bool",
        "disable_mqtt": "bool",
        "mqtt_host": "str",
        "mqtt_user": "str",
        "mqtt_pass": "str",
        "mqtt_tls": "bool",
        "mqtt_tls_accept_invalid_certs": "bool",
        "mqtt_namespace": "str",
        "timezone": "str"
    },
    "url": "https://github.com/matt-FFFFFF/hassio-addon-repository",
    "image": "mattffffff/teslamate-{arch}",
    "ingress": true,
    "ingress_port": 8099
  }